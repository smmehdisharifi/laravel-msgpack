{
    "name": "smmehdisharifi/laravel-msgpack",
    "version": "1.0.0",
    "description": "Laravel integration for MessagePack",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehdi Sharifi",
            "email": "smmehdisharifi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "rybakit/msgpack": "^0.7"
    },
    "require-dev": {
        "orchestra/testbench": "^8.15"
    },
    "autoload": {
        "psr-4": {
            "SmMehdiSharifi\\LaravelMsgpack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmMehdiSharifi\\LaravelMsgpack\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmMehdiSharifi\\LaravelMsgpack\\MsgpackServiceProvider"
            ],
            "aliases": {
                "Msgpack": "SmMehdiSharifi\\LaravelMsgpack\\Facades\\Msgpack"
            }
        }
    },
    "minimum-stability": "stable"
}
